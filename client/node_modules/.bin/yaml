#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hacker-tech/Desktop/MERN/week-6-test-debug-assignment-Sir-JamesMuritu/client/node_modules/yaml/node_modules:/home/hacker-tech/Desktop/MERN/week-6-test-debug-assignment-Sir-JamesMuritu/client/node_modules:/home/hacker-tech/Desktop/MERN/week-6-test-debug-assignment-Sir-JamesMuritu/node_modules:/home/hacker-tech/Desktop/MERN/node_modules:/home/hacker-tech/Desktop/node_modules:/home/hacker-tech/node_modules:/home/node_modules:/node_modules:/home/hacker-tech/Desktop/MERN/week-6-test-debug-assignment-Sir-JamesMuritu/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hacker-tech/Desktop/MERN/week-6-test-debug-assignment-Sir-JamesMuritu/client/node_modules/yaml/node_modules:/home/hacker-tech/Desktop/MERN/week-6-test-debug-assignment-Sir-JamesMuritu/client/node_modules:/home/hacker-tech/Desktop/MERN/week-6-test-debug-assignment-Sir-JamesMuritu/node_modules:/home/hacker-tech/Desktop/MERN/node_modules:/home/hacker-tech/Desktop/node_modules:/home/hacker-tech/node_modules:/home/node_modules:/node_modules:/home/hacker-tech/Desktop/MERN/week-6-test-debug-assignment-Sir-JamesMuritu/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../yaml/bin.mjs" "$@"
else
  exec node  "$basedir/../yaml/bin.mjs" "$@"
fi
